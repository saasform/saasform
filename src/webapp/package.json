{
  "name": "@saasform/webapp",
  "private": true,
  "version": "0.5.1",
  "description": "Saasform webapp",
  "author": "Saasform Team <hello@saasform.dev> (https://saasform.dev)",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "ts-standard --fix",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node --require ts-node/register ./node_modules/typeorm/cli.js schema:drop -c test && node --require ts-node/register ./node_modules/typeorm/cli.js migration:run -c test && TZ=UTC jest --silent --runInBand --config ./test/jest-e2e.json",
    "test:e2elog": "node --require ts-node/register ./node_modules/typeorm/cli.js schema:drop -c test && node --require ts-node/register ./node_modules/typeorm/cli.js migration:run -c test && TZ=UTC jest --runInBand --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "@bugout/humbug": "^0.0.3",
    "@nestjs-query/query-graphql": "^0.21.2",
    "@nestjs-query/query-typeorm": "^0.21.2",
    "@nestjs/common": "^7.6.6",
    "@nestjs/config": "^0.6.2",
    "@nestjs/core": "^7.5.1",
    "@nestjs/graphql": "^7.9.8",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.8.1",
    "@nestjs/typeorm": "^7.1.5",
    "@sendgrid/mail": "^7.4.2",
    "@types/passport-http-bearer": "^1.0.37",
    "add": "^2.0.6",
    "apollo-server-express": "^2.19.2",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "dataloader": "^2.0.0",
    "ec-key": "^0.0.4",
    "google-auth-library": "^7.0.2",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.6.0",
    "graphql-subscriptions": "^1.1.0",
    "helmet": "^4.4.1",
    "js-yaml": "^4.1.0",
    "killbill": "^0.1.0",
    "liquidjs": "^9.20.1",
    "markdown-it": "^12.3.2",
    "mysql": "^2.18.1",
    "parse-domain": "^3.0.3",
    "passport": "^0.4.1",
    "passport-azure-ad": "0x0ece/passport-azure-ad",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2-oidc": "^0.0.2",
    "passport-saml": "^2.2.0",
    "passport-totp": "^0.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "stripe": "^8.135.0",
    "swagger-ui-express": "^4.1.6",
    "tld-extract": "^2.0.1",
    "typeorm": "^0.2.32",
    "urlsafe-base64": "^1.0.0",
    "validator": "^13.5.2",
    "yarn": "^1.22.10"
  },
  "resolutions": {
    "killbill/axios": "^0.21.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.22",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "pinst": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "ts-standard": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "ts-standard": {
    "project": "./tsconfig.json",
    "ignore": [
      "themes/**/*.js"
    ]
  }
}
