  <section id="pricing" class="section is-medium section-feature-grey">
    <div class="container">
      <div class="content has-text-centered">
        <h2 class="title is-2">{{ pricing_title | formatDot }}</h2>
  
        <div class="field">
          Monthly
          <input id="switchRoundedInfo" type="checkbox" name="switchRoundedInfo" class="switch is-rounded is-info" checked="checked">
          Annually
        </div>
  
        <h3 class="subtitle is-5 is-muted has-text-weight-normal">
          {% if pricing_text != '' %}
            {{ pricing_text | formatNoOrphanDot }}
          {% elsif pricing_free_trial > 0 %}
            All plans start with a {{ pricing_free_trial }} day free trial.
            {% if pricing_credit_card_required == false %}
              <br/>No credit card required to sign up.
            {% endif %}
          {% endif %}
        </h3>
      </div>
  
      <div class="pricing-wrap">
      {% for item in pricing_plans %}
        <div class="feature-card is-pricing has-text-centered">
          <h3 class="plan-name">{{ item.name }}</h3>
          <div class="price">
            {% if item.price_text %}
              {{ item.price_text }}
            {% else %}
              <span>$</span>{{ item.price_year }}<span>/mo</span>
            {% endif %}
          </div>
  
          <ul>
          {% for feature in item.features %}
            <li>
              {{ feature }}
            </li>
          {% endfor %}
          </ul>
  
          {% if item.primary %}
          <a class="button is-fullwidth is-bold is-primary">
          {% else %}
          <a class="button is-fullwidth is-bold">
          {% endif %}
            {{ item.button }}
          </a>
        </div>
      {% endfor %}
      </div>
    </div>
  </section>
  
  <section id="faq" class="section section-light-grey is-medium">
  </section>
  
